{
  "AWSEBDockerrunVersion": 2,
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "rdkitserver",
      "image": "timdudgeon/rdkitserver",
      "environment": [
        {
          "name": "SERVER_NAME",
          "value": "localhost"
        },
	{
          "name": "SERVER_PORT",
          "value": "8000"
        }
      ],
      "essential": true,
      "memory": 500,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8000
        }
      ],
      "mountPoints": []
    }
  ]
}
